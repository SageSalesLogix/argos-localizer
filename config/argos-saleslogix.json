{
    "export": {
        "transform": "transforms/argos-saleslogix-export.xslt",
        "path": "localization/argos-saleslogix/slx-mobile-strings-beta1.xml",
        "split": true
    },
    "import": {
        "transform": "transforms/argos-saleslogix-import.xslt",
        "template": "templates/argos-saleslogix.erb",
        "map": {
            "de": {
                "in": [
                        "localization/argos-saleslogix/slx-mobile-strings-beta1-format-de.xml",
                        "localization/argos-saleslogix/slx-mobile-strings-beta1-text-de.xml"
                    ],
                "out": "localization/argos-saleslogix/de.js"
            },
            "fr": {
                "in": [
                        "localization/argos-saleslogix/slx-mobile-strings-beta1-format-fr.xml",
                        "localization/argos-saleslogix/slx-mobile-strings-beta1-text-fr.xml"
                    ],
                "out": "localization/argos-saleslogix/fr.js"
            },
            "it": {
                "in": [
                        "localization/argos-saleslogix/slx-mobile-strings-beta1-format-it.xml",
                        "localization/argos-saleslogix/slx-mobile-strings-beta1-text-it.xml"
                    ],
                "out": "localization/argos-saleslogix/it.js"
            },
            "ru": {
                "in": [
                        "localization/argos-saleslogix/slx-mobile-strings-beta1-format-ru.xml",
                        "localization/argos-saleslogix/slx-mobile-strings-beta1-text-ru.xml"
                    ],
                "out": "localization/argos-saleslogix/ru.js"
            },
            "en-GB": {
                "in": "localization/argos-saleslogix/slx-mobile-strings-beta1-format-en-GB.xml",
                "out": "localization/argos-saleslogix/en-GB.js"
            }
        }
    },
    "projects": [{
        "name": "Argos SDK",
        "path": "argos-sdk/src",
        "alias": "argos-sdk"
    },{
        "name": "SalesLogix Mobile",
        "path": "products/argos-saleslogix/src",
        "alias": "argos-saleslogix"
    }]
}
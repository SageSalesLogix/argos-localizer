{
    "export": {
        "transform": "transforms/argos-saleslogix-export.xslt",
        "path": "localization/argos-saleslogix/slx-mobile-strings-beta1.xml",
        "split": true,
        "legacy": true
    },
    "import": {
        "transform": "transforms/argos-saleslogix-import.xslt",
        "template": "templates/argos-saleslogix-v1.2.erb",
        "map": {
            "de": {
                "in": "localization/argos-saleslogix/slx-mobile-strings-beta1.de.xml",
                "out": "localization/argos-saleslogix/de.js"
            },
            "fr": {
                "in": "localization/argos-saleslogix/slx-mobile-strings-beta1.fr.xml",
                "out": "localization/argos-saleslogix/fr.js"
            },
            "it": {
                "in": "localization/argos-saleslogix/slx-mobile-strings-beta1.it.xml",
                "out": "localization/argos-saleslogix/it.js"
            },
            "ru": {
                "in": "localization/argos-saleslogix/slx-mobile-strings-beta1.ru.xml",
                "out": "localization/argos-saleslogix/ru.js"
            },
            "en-GB": {
                "in": "localization/argos-saleslogix/slx-mobile-strings-beta1-format.en-GB.xml",
                "out": "localization/argos-saleslogix/en-GB.js"
            }
        }
    },
    "projects": [{
        "name": "Argos SDK",
        "path": "argos-sdk/src",
        "alias": "argos-sdk"
    },{
        "name": "SalesLogix Mobile",
        "path": "products/argos-saleslogix/src",
        "alias": "argos-saleslogix"
    }]
}